{
  "name": "palworld-overwolf",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && bunx --bun vite build && bun run scripts/copy-overwolf.ts",
    "clean": "rimraf dist node_modules",
    "dev": "bunx --bun vite --port 3004",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "release": "node scripts/release-overwolf.mjs"
  },
  "dependencies": {
    "react": "catalog:",
    "react-dom": "catalog:",
    "@repo/lib": "workspace:*",
    "@repo/ui": "workspace:*"
  },
  "devDependencies": {
    "@overwolf/ow-cli": "catalog:",
    "@overwolf/types": "catalog:",
    "@repo/eslint-config": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react-swc": "catalog:",
    "autoprefixer": "catalog:",
    "postcss": "catalog:",
    "reflect-metadata": "0.2.2",
    "tailwindcss": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:"
  }
}